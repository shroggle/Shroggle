package com.shroggle.util.resource.resize;

/**
 * @author Artem Stasuk
 */
public class ResourceResizeException extends RuntimeException {

    public ResourceResizeException(final String message) {
        super(message);
    }

    public ResourceResizeException(final Throwable cause) {
        super(cause);
    }

    public ResourceResizeException(final String message, final Throwable cause) {
        super(message, cause);
    }

}
