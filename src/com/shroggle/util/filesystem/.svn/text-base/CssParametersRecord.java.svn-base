package com.shroggle.util.filesystem;

import com.shroggle.presentation.site.cssParameter.CssParameter;
import com.shroggle.entity.MenuStyleType;
import com.shroggle.entity.ItemType;

import java.util.ArrayList;
import java.util.List;

/**
 * @author Stasuk Artem
 */
class CssParametersRecord {

    private ItemType itemType;
    private MenuStyleType.OldValues menuStyleType;
    private List<CssParameter> cssParameters = new ArrayList<CssParameter>();

    public MenuStyleType.OldValues getMenuStyleType() {
        return menuStyleType;
    }

    public void setMenuStyleType(MenuStyleType.OldValues menuStyleType) {
        this.menuStyleType = menuStyleType;
    }

    public List<CssParameter> getCssParameters() {
        return cssParameters;
    }

    public void setCssParameters(List<CssParameter> cssParameters) {
        this.cssParameters = cssParameters;
    }

    public ItemType getItemType() {
        return itemType;
    }

    public void setItemType(ItemType itemType) {
        this.itemType = itemType;
    }
}
