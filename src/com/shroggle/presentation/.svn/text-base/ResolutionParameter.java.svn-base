package com.shroggle.presentation;

/**
 * @author Artem Stasuk
 */
final public class ResolutionParameter {

    public ResolutionParameter(final String name, final Object value) {
        this.name = name;
        this.value = value;
    }

    public Object getValue() {
        return value;
    }

    public String getName() {
        return name;
    }

    private final String name;
    private final Object value;

}
