package com.shroggle.presentation.site;

import org.directwebremoting.annotations.DataTransferObject;

/**
 * Author: dmitry.solomadin
 * Date: 20.03.2009
 */
@DataTransferObject
public class YourFormTableFormItemInfo {

    private String formItemName;

    private String fieldName;

    private String itemFieldType;

    private String itemDescription;

    private String itemDefaultInstruction;

    public String getItemDefaultInstruction() {
        return itemDefaultInstruction;
    }

    public void setItemDefaultInstruction(String itemDefaultInstruction) {
        this.itemDefaultInstruction = itemDefaultInstruction;
    }

    public String getFormItemName() {
        return formItemName;
    }

    public void setFormItemName(String formItemName) {
        this.formItemName = formItemName;
    }

    public String getFieldName() {
        return fieldName;
    }

    public void setFieldName(String fieldName) {
        this.fieldName = fieldName;
    }

    public String getItemFieldType() {
        return itemFieldType;
    }

    public void setItemFieldType(String itemFieldType) {
        this.itemFieldType = itemFieldType;
    }

    public String getItemDescription() {
        return itemDescription;
    }

    public void setItemDescription(String itemDescription) {
        this.itemDescription = itemDescription;
    }
}
