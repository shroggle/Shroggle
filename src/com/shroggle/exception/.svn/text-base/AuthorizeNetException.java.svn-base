package com.shroggle.exception;

/**
 * @author Balakirev Anatoliy
 */
public class AuthorizeNetException extends RuntimeException {

    public AuthorizeNetException() {
    }

    public AuthorizeNetException(String message) {
        super(message);
    }

    public AuthorizeNetException(String message, Throwable cause) {
        super(message, cause);
    }

    public AuthorizeNetException(Throwable cause) {
        super(cause);
    }
}
