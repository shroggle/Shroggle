package com.shroggle.logic.advancedSearch.resultsNumber;

import java.util.List;
import java.util.ArrayList;

/**
 * @author dmitry.solomadin
 */
public class AdvancedSearchGalleryPair {

    private int advancedSearchId;

    private int galleryId;

    private List<CachedSearchCriteria> cachedSearchCriterias = new ArrayList<CachedSearchCriteria>();

    private List<Integer> fullFilledFormsHashCodesFetch = new ArrayList<Integer>();

    public List<Integer> getFullFilledFormsHashCodesFetch() {
        return fullFilledFormsHashCodesFetch;
    }

    public void setFullFilledFormsHashCodesFetch(List<Integer> fullFilledFormsHashCodesFetch) {
        this.fullFilledFormsHashCodesFetch = fullFilledFormsHashCodesFetch;
    }

    public int getAdvancedSearchId() {
        return advancedSearchId;
    }

    public void setAdvancedSearchId(int advancedSearchId) {
        this.advancedSearchId = advancedSearchId;
    }

    public int getGalleryId() {
        return galleryId;
    }

    public void setGalleryId(int galleryId) {
        this.galleryId = galleryId;
    }

    public List<CachedSearchCriteria> getCachedSearchCriterias() {
        return cachedSearchCriterias;
    }

    public void setCachedSearchCriterias(List<CachedSearchCriteria> cachedSearchCriterias) {
        this.cachedSearchCriterias = cachedSearchCriterias;
    }

    public void addCachedSearchCriteria(CachedSearchCriteria cachedSearchCriteria) {
        cachedSearchCriterias.add(cachedSearchCriteria);
    }

    public void removeCachedSearchCriteria(CachedSearchCriteria cachedSearchCriteria){
        cachedSearchCriterias.remove(cachedSearchCriteria);
    }

}

