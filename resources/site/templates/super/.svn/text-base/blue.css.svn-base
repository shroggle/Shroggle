body {
    color: slategray;
}

a {
    font-weight: bold;
    color: #4d6498;
    font-size: 10pt;
}

.advSearchLink {
    text-decoration:none;
    border-bottom: 1px dashed #4d6498;
}

#topPane {
    background-color: #4D6498;
    border-bottom: 1px dotted #B5C4DB;
    COLOR: #ffffff;
}

.topDiv {
    border-bottom: 7px solid #B5C4DB;
}

#topPane, #topPane a, .bottomPane, .bottomPane a{
    color: white;
}

#middlePaneRight {
    border-left: 1px dotted #4D6498;
}

.bottomPane {
    background-color: #4D6498;
}

/*Blog visual item tyles. Beginning*/

.blogPostText p, .blogPostText div, .blogPostCommentText p, .blogPostCommentText div {
    font-size: 10pt;
}

.blogName {
    font-weight: bold;
    font-style: italic;
    font-size: 20pt;
    padding: 5px 0;
    color: #5072a6;
}

.addBlogPost {
    padding-bottom: 10px;
}

.blogPostTitle {
    display: block;
    font-size: 14pt;
    color: #4D6498;
    padding-top: 20px;
    padding-bottom: 20px;
    border-top: 1px dotted #dedede;
}

.blogPostInfo span, .blogPostCommentInfo span {
    font-size: 8pt;
    font-style: italic;
    padding-left: 20px;
}

.blogPostLinks span, .blogPostCommentLinks span {
    padding-right: 20px;
    font-size: 9pt;
    font-weight: bold;
}

.blogPostLinks {
    padding: 5px 0;
}

.addBlogPostComment {
    font-weight: bold;
    font-size: 9pt;
    display: block;
    padding-bottom: 10px;
}

.blogPostComments {
    padding-left: 20px;
    padding-top: 10px;
    padding-bottom: 15px;
    border: 1px dotted #dedede;

}

.blogPostCommentsShowHide {
    font-weight: bold;
    font-size: 9pt;
}

/*Blog visual item tyles. The end*/

/* - Image visual item tyles. Beginning*/

.widgetIMAGEStyle {
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif !important;
}

.imageTitle {
    padding-top: 10px;
    padding-bottom: 5px;
    font-size: 1.2em;
    font-weight: bold;
}

.imageImage {
    padding-top: 7px;
    padding-bottom: 7px;
}

.imageDescription {
    padding-top: 10px;
    padding-bottom: 10px;
    font-size: 1em;
}

/* - Image visual item tyles. The end*/

