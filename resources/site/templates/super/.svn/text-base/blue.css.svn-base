/*registr-login beginning*/
/*login form style*/
.widgetLOGINStyle {
    padding-left: 5px;
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
    color: #5072ab;
}

.loginFormHeader {
    font-size: 1.1em;
    padding-bottom: 15px;
    font-weight: bold;
    color: #4d6498;

}

.loginForm {
    font-weight: bold;
    line-height: 1.4em;
}

.loginFormLabel {
    font-size: 10pt;
    font-weight: bold;
    padding-top: 5px;
}

.loginForm a:link {
    color: #333333;
    text-decoration: none;
}

.loginForm a:hover {
    color: black;
    text-decoration: underline;
}

a:link.loginFormReturnToRegLink {
    color: #333333;
    text-decoration: underline;
    font-weight: bold;

}

a:link.loginFormRegLink {
    color: #4d6498;
}

#topPane    .widgetLOGINStyle {
    padding-left: 5px;
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
    color: #dedede;
}

#topPane  .loginFormHeader {
    font-size: 1.1em;
    padding-bottom: 15px;
    font-weight: bold;
    color: #fafafa;

}

#topPane   .loginForm a:link {
    color: #dedede;
    text-decoration: none;
}

#topPane  .loginForm a:hover {
    color: #fff;
    text-decoration: underline;
}

#topPane  a:link.loginFormReturnToRegLink {
    color: #fafafa;
    text-decoration: underline;
    font-weight: bold;

}

#topPane  a:link.loginFormRegLink {
    color: #fafafa;
}

/*login form style the end */
