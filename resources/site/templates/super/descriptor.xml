<template>
    <name>Blogger twice template</name>
    <thumbnail>tn_purple.png</thumbnail>
    <order>20</order>

    <layout thumbnailFile="header_main_left.gif">
        <name>Header, left and main panels</name>
        <file>header_main_leftmenu.html</file>


    </layout>
    <layout thumbnailFile="header_main.gif">
