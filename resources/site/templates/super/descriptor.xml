    <layout thumbnailFile="header_main_footer.gif" useForDefault="true">
        <name>Header, main panel and footer</name>
        <file>header_main_footer.html</file>
          <pattern type="REGISTRATION">
            <position type="REGISTRATION" position="1"/>
        </pattern>
        <pattern type="BLANK">
            <position position="1"/>
        </pattern>
        <pattern type="BLOG">
            <position type="BLOG" position="1"/>
        </pattern>
