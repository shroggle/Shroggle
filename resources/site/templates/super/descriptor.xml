        </pattern>
        <pattern type="LOGIN">
            <position type="LOGIN" position="1"/>
        </pattern>
        <pattern type="SLIDE">
            <position type="SLIDESHOW" position="1"/>
        </pattern>
        <pattern type="VIDEO">
            <position type="VIDEO" position="1"/>
        </pattern>
    </layout>
