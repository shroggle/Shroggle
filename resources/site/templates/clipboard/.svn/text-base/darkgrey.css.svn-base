@charset "utf-8";
/* CSS Document */
body {
	background-color:#333;
      font-size: 10pt;
    font-family: Arial, Helvetica, sans-serif;
    color:#333;
}

.topPane,  .bottomPane, .shroggleLogo  {
    color: #fff;
}
.topPane a:link, .topPane a:visited, .bottomPane a:link, .bottomPane a:visited {
   color:#eee;
}
.topPane a:hover, .topPane a:active, .bottomPane a:active, .bottomPane a:hover {
   color:#fff;
}
.middlePaneContainerTop {
	background-image:url(<!-- TEMPLATE_RESOURCE -->/images/writingpadTop.png);
	background-repeat:no-repeat;

}

.middlePaneContainerBottom {
	background-image:url(<!-- TEMPLATE_RESOURCE -->/images/writingpadBottom.png);
	background-repeat:no-repeat;

}
.middlePane {

	background-image: url(<!-- TEMPLATE_RESOURCE -->/images/writingpadMiddle.png);
	background-repeat:repeat-y;
}

