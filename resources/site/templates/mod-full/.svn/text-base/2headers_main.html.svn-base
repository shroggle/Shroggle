<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title><!-- PAGE_TITLE --></title>
    <!-- PAGE_HEADER -->
    <style type="text/css">
body, div, p, table, tr, td {
	margin: 0;
	padding: 0;
}
table, tr, td {
	vertical-align: top;
}
.header {
    width: 975px;
	min-height: 70px;
	height: auto;
	margin:auto;

}
.headleftTd {
	width: 50%;
	min-width: 300px;
}
.headrightTd {
	width: 50%;
	min-width: 300px;
}
.headleft {
	width: 100%;
	min-height: 70px;
	height: auto;
}
.headright {
	width: 100%;
	min-height: 70px;
	height: auto;
}
#break {
	width: 100%;
	height: 5px;
	border-top: 2px solid white;
	border-bottom: 2px solid white;
	clear: both;
}
.mainarea {
	clear:both;
}
.content {
    width: 975px;
	height: auto;
	min-height: 600px;
	margin:auto;
	overflow:hidden;
}

</style>
    </head>
    <body>
<div class="headercontainer">
		<div class="header">
		<table cellpadding="0" cellspacing="0">
				<tr valign="top">
				<td class="headleftTd">
						<div class="headleft mediaBlock"></div>
					</td>
				<td class="headrightTd">
						<div class="headright mediaBlock"></div>
					</td>
			</tr>
			</table>
	</div>
	</div>
<div id="break"></div>
<div class="mainarea">
		<div class="content mediaBlock"> </div>
	</div>

</body>
</html>
