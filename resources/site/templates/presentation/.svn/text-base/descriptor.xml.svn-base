        </pattern>
        <pattern type="LOGIN">
            <position type="LOGIN" position="2"/>
        </pattern>
        <pattern type="SLIDE">
            <position type="SLIDESHOW" position="2"/>
        </pattern>
        <pattern type="VIDEO">
            <position type="VIDEO" position="2"/>
        </pattern>
        
    </layout>
    <layout thumbnailFile="3headers_2footers.gif">
        <name>3 Headers, 2 Footers</name>
        <file>3headers_main_2footers.html</file>

    </layout>
    <layout thumbnailFile="2headers_2footers_2content.gif">
        <name>3 Headers, 2 Footers, 2 Equal Content Areas</name>
        <file>3headers_2main_2footers.html</file>
       
    </layout>

