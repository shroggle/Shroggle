<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<template>
    <name>Template with corners around</name>
    <thumbnail>tn_claret.png</thumbnail>
    <order>50</order>

    <layout thumbnailFile="2headers.gif" useForDefault="true">
        <name>2 Headers</name>
        <file>2headers_main.html</file>
        <pattern type="REGISTRATION">
            <position type="REGISTRATION" position="2"/>
        </pattern>
        <pattern type="BLANK">
            <position position="2"/>
        </pattern>
        <pattern type="BLOG">
            <position type="BLOG" position="2"/>
        </pattern>
        <pattern type="CALENDAR">
            <position type="CALENDAR" position="2"/>
        </pattern>
