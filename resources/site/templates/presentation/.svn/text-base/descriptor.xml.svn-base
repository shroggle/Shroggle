        </pattern>
        <pattern type="DISPLAY_REGISTRATION">
            <position type="DISPLAY_REGISTRATION" position="2"/>
        </pattern>
        <pattern type="FORUM">
            <position type="FORUM" position="2"/>
        </pattern>
        <pattern type="GALLERY">
            <position type="GALLERY" position="2"/>
        </pattern>
        <pattern type="HOME">
            <position type="BLOG_SUMMARY" position="2"/>
        </pattern>
        <pattern type="E_COMMERCE">
