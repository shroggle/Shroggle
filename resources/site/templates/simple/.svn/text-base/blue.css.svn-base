.widgetVIDEOStyle {
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
    font-size: 10pt;
    color: #333333;
}

/*video item style. the end*/

/*login form style*/
.widgetLOGINStyle {
    padding-left: 15px;
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
    font-size: 10pt;
    color: navy;
}

.widgetLOGINStyle td {
    font-size: 10pt;
    color: navy;
    padding: 5px;

}

.loginForm {
    font-size: 11pt !important;
    padding: 5px;
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
}

.loginForm a:link {
    color: #6372ED;
    text-decoration: underline;
    font-size: 10pt;
}

.loginForm a:hover {
    color: #c54524;

}

.loginFormHeader {
    font-size: 12pt;
    padding-bottom: 15px;
    font-weight: bold;
    color: #c54524;

}

.loginFormLabel {
    font-size: 10pt;
    padding-top: 5px;
}

a:link.loginFormReturnToRegLink {
    color: #6372ed !important;
    text-decoration: underline;
    font-size: 10pt;
}

a:link.loginFormRegLink {
    color: #c54524 !important;
}

/*login form style the end */
