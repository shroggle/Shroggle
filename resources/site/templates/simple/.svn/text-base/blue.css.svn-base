    font-weight: bold;
}

.threadNameLink:hover {
    color: red;
}

.postList {
    border: 1px solid dimgray;
}

.postListHeading {
    background-color: #dedfdf;
}

.forumFont {
    font-family: Tahoma, sans-serif;
}

.postRow {
    background-color: #ffffff;
}

.subForumListHeadingRow {
