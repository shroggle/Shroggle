<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>

    <title><!-- PAGE_TITLE --></title>
    <!-- PAGE_HEADER -->
    <style type="text/css">
        body, div, p, img {
            margin: 0;
            padding: 0;
        }

        }
        body {

            font-size: 10pt;
            font-family: Arial, Helvetica, sans-serif;
        }

        #content {
            margin: 20px auto;
            width: 800px;
            height: auto;
            overflow: hidden;
        }

        #topPane {
            width: 800px;
            min-height: 85px;
            height: auto;
            display: table-cell;
            vertical-align: bottom;

        }

        .headleft {
            width: 50%;
            min-width: 200px;
            height: 100%;
        }

        .headright {
            width: 50%;
            min-width: 200px;
            height: 100%;

        }

        #middlePane {
            width: 100%;
            min-height: 400px;
            height: auto;
            clear: both;
            background-color: white;
              color:#333;
        }

        .middleTop {
            clear: both;
            width: 100%;
            min-height: 20px;
            height: auto;

        }

        .middleLeft {
            width: 50%;
            min-width: 200px;
            min-height: 200px;
            height: auto;

        }

        .middleRight {
            min-height: 200px;
            height: auto;
            width: 50%;
            min-width: 200px;

        }

        .bottomPane {
            width: 800px;
            min-height: 20px;
            height: auto;
            clear: both;

        }

        .subbottomPane {
            width: 800px;
            min-height: 20px;
            height: auto;
            clear: both;

        }

      
    </style>

</head>
<body>
<div id="content">
    <div id="topPane">
        <table width="800px" cellpadding="0" cellspacing="0">
            <tr valign="middle">
                <td class="headleft mediaBlock"></td>
                <td class="headright mediaBlock"></td>
            </tr>
        </table>

    </div>

    <div id="middlePane">
        <div class="middleTop mediaBlock"></div>
        <table width="800px" cellpadding="0" cellspacing="0">
            <tr valign="top">
                <td class="middleLeft mediaBlock"></td>
                <td class="middleRight mediaBlock"></td>
            </tr>
        </table>
    </div>

    <div class="bottomPane mediaBlock"></div>
    <div class="subbottomPane mediaBlock"></div>

</div>


</body>
</html>
